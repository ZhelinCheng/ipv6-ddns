{
  "name": "hpv_subscribe",
  "version": "1.1.0",
  "description": "HPV微信公众号订阅",
  "main": "src/app.ts",
  "scripts": {
    "start:dev": "nodemon -e ts, js",
    "build": "npm run lint && rimraf dist && tsc",
    "format": "prettier --write ./src/**/*.ts",
    "lint": "eslint ./src/**/*.ts --ext .ts",
    "prepare-commit-msg": "prepare-commit-msg",
    "prepare": "husky install",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "test": "jest --forceExit --verbose --detectOpenHandles --coverage",
    "watch-test": "npm run test -- --watchAll",
    "puppet-install": "node node_modules/.bin/wechaty-puppet-install",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhelinCheng/wechat-anti-revoke.git"
  },
  "keywords": [
    "hpv",
    "wechat"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZhelinCheng/wechat-anti-revoke/issues"
  },
  "homepage": "https://github.com/ZhelinCheng/wechat-anti-revoke#readme",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "commitlint": "16.2.1",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "coveralls": "3.1.1",
    "eslint": "8.9.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@types/async": "^3.2.12",
    "@types/cron": "^1.7.3",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/level": "^6.0.0",
    "@types/pino": "^7.0.5",
    "@types/qrcode-terminal": "^0.12.0",
    "@types/qs": "^6.9.7",
    "@types/wuzzy": "^0.1.0",
    "@types/xml2js": "^0.4.9",
    "async": "^3.2.3",
    "axios": "^0.26.0",
    "cheerio": "^1.0.0-rc.10",
    "cron": "^1.8.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.0",
    "level": "^7.0.1",
    "mint-filter": "^3.0.1",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1",
    "qrcode-terminal": "^0.12.0",
    "qs": "^6.10.3",
    "wuzzy": "^0.1.8",
    "xml2js": "^0.4.23"
  }
}
